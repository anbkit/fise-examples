{
    "$schema": "https://turbo.build/schema.json",
    "globalEnv": [
        "VITE_API_BASE"
    ],
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**",
                "build/**"
            ]
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "lint": {
            "outputs": []
        },
        "test": {
            "dependsOn": [
                "build"
            ],
            "outputs": []
        }
    }
}